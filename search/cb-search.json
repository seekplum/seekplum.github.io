---
layout: null
---
{
	"code" : 0,
	"data" : [
      {% for post in site.posts %}
        {
            "title" : "{{ post.title }} - {% for tag in post.tags %}{% if forloop.rindex != 1 %}{{ tag }}_{% else %}{{ tag }}{% endif %}{% endfor %} - {{ post.content | replace:'	',''  | strip_html | strip_newlines | replace:'"','' | replace:'\','' }}",
            "url" : "{{ post.url }}"
        }{% if forloop.rindex != 1  %},{% endif %}
      {% endfor %}
	]
}